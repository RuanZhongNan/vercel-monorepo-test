{
	"name": "@ruan-cat/vercel-deploy-tool",
	"private": true,
	"version": "1.0.0",
	"description": "阮喵喵打算做一个简单的包，用于实现部署。目前先考虑vercel平台。",
	"scripts": {
		"—build-not-use-for-now": "tsc",
		"start": "node ./dist/index.js",
		"run": "vite-node ./src/index.ts --files",
		"test:config": "vite-node ./tests/config.test.ts --files",
		"test:vitest": "vitest --ui --watch"
	},
	"type": "module",
	"main": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"__types": "./dist/index.d.ts",
			"__import": "./dist/index.mjs",
			"__require": "./dist/index.cjs",
			"types": "./src/index.ts",
			"import": "./src/index.ts"
		},
		"./*": "./src/*"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"c12": "^1.11.2",
		"consola": "^3.2.3",
		"execa": "^9.3.1",
		"pathe": "^1.1.2",
		"vercel": "^34.2.7"
	},
	"devDependencies": {
		"@types/node": "^22.5.1",
		"@vitest/ui": "^2.0.5",
		"vitest": "^2.0.5"
	}
}