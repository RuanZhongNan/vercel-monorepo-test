{
	"name": "@ruan-cat-vercel-monorepo-test/vuepress-preset-config",
	"private": false,
	"version": "0.1.0",
	"description": "用于给大多数的vuepress项目提供一个预设的配置文件",
	"author": "ruan-cat",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"build": "vite build",
		"build:watch": "vite build --watch",
		"clear": "rimraf dist && rimraf .turbo && rimraf .vercel",
		"docs:update-package": "pnpm dlx vp-update",
		"test": "echo test"
	},
	"main": "./dist/vuepress-preset-config.js",
	"module": "./dist/vuepress-preset-config.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"require": "./dist/vuepress-preset-config.umd.cjs",
			"module": "./dist/vuepress-preset-config.js",
			"import": "./dist/vuepress-preset-config.js",
			"types": "./src/index.ts"
		},
		"./package.json": "./package.json",
		"./dist/style": {
			"default": "./dist/style.css"
		},
		"./*": "./*"
	},
	"typesVersions": {
		"*": {
			"*": [
				"./dist/*",
				"./*"
			]
		}
	},
	"files": [
		"dist"
	],
	"devDependencies": {
		"@types/node": "^20.14.9",
		"@types/rollup-plugin-visualizer": "^4.2.4"
	},
	"dependencies": {
		"@vuepress/bundler-vite": "2.0.0-rc.14",
		"rollup-plugin-visualizer": "^5.12.0",
		"vuepress": "2.0.0-rc.14",
		"vuepress-theme-hope": "2.0.0-rc.50"
	}
}