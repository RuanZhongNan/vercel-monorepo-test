{
	"name": "@ruan-cat/vuepress-preset-config",
	"version": "0.1.5",
	"description": "用于给大多数的vuepress项目提供一个预设的配置文件。",
	"type": "module",
	"main": "./dist/vuepress-preset-config.js",
	"types": "./src/index.ts",
	"homepage": "https://github.com/RuanZhongNan/vercel-monorepo-test/tree/main/packages/vuepress-preset-config",
	"bugs": {
		"url": "https://github.com/RuanZhongNan/vercel-monorepo-test/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/RuanZhongNan/vercel-monorepo-test.git",
		"directory": "packages/vuepress-preset-config"
	},
	"scripts": {
		"build": "vite build",
		"build:watch": "vite build --watch",
		"clear": "rimraf dist .turbo .vercel .cache .temp",
		"docs:update-package": "pnpm dlx vp-update",
		"rm:node_modules": "rimraf node_modules",
		"publish": "pnpm build"
	},
	"exports": {
		".": {
			"import": "./dist/vuepress-preset-config.js",
			"types": "./src/index.ts"
		},
		"./src/*": "./src/*"
	},
	"keywords": [
		"vuepress"
	],
	"author": {
		"name": "ruan-cat",
		"email": "1219043956@qq.com",
		"url": "https://github.com/RuanZhongNan"
	},
	"license": "MIT",
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/",
		"tag": "beta"
	},
	"typesVersions": {
		"*": {
			"*": [
				"./dist/*",
				"./*"
			]
		}
	},
	"files": [
		"dist",
		"src"
	],
	"devDependencies": {
		"@types/node": "^20.16.6",
		"@types/rollup-plugin-visualizer": "^4.2.4",
		"sass-embedded": "^1.79.3"
	},
	"dependencies": {
		"@vuepress/bundler-vite": "2.0.0-rc.15",
		"rollup-plugin-visualizer": "^5.12.0",
		"vuepress": "2.0.0-rc.15",
		"vuepress-plugin-search-pro": "2.0.0-rc.54",
		"vuepress-theme-hope": "2.0.0-rc.56"
	}
}